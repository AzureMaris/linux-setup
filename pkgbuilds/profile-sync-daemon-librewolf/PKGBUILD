pkgname=profile-sync-daemon-librewolf
_pkgname=$pkgname
pkgver=1
pkgrel=1
pkgdesc='Adds librewolf profile for profile sync daemon'
arch=(any)
depends=(profile-sync-daemon librewolf)
provides=($_pkgname)
conflicts=($_pkgname)

package() {
   install -Dm644 "$srcdir/../librewolf" "$pkgdir/usr/share/psd/browsers/librewolf"
}
