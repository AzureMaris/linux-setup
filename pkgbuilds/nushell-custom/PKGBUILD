pkgname=nushell-custom
_pkgname=$pkgname
pkgver=1
pkgrel=1
pkgdesc='Enables some experimental options'
arch=(any)
depends=(nushell)
provides=($_pkgname)
conflicts=($_pkgname $_pkgname-bin $_pkgname-git)
install=${_pkgname}.install

package() {
   install -Dm755 /dev/null "$pkgdir/usr/bin/nu-custom"
   install -Dm755 "$srcdir/../nu-custom" "$pkgdir/usr/bin/nu-custom"
}
